{
    "name": "giosf/form-builder",
    "description": "A laravel form builder with components",
    "type": "library",
    "license": "proprietary",
    "autoload": {
        "psr-4": {
            "Giosf\\FormBuilder\\": "src/"
        }
    },
	"authors": [
		{
			"name": "Giovanni Stroppa",
			"email": "giosfdev@gmail.com"
		}
	],
	"repositories": [
		{
			"type": "git",
			"url":	"git@github.com:giosf/form-builder.git"
		}
	],
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.3"
    },
    "extra": {
		"laravel": {
			"providers": [
				"Giosf\\FormBuilder\\ServiceProvider"
			]
		}
	},
    "require-dev": {
        "phpunit/phpunit": "^11.3"
    }
}
